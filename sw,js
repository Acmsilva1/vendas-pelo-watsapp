const CACHE_NAME = 'delicias-ju-v6';
// Adicionando o prefixo do novo repositÃ³rio nos assets
const assets = [
  '/vendas-pelo-watsapp/',
  '/vendas-pelo-watsapp/index.html',
  '/vendas-pelo-watsapp/manifest.json'
];

self.addEventListener('install', e => {
  e.waitUntil(
    caches.open(CACHE_NAME).then(cache => {
      return cache.addAll(assets);
    })
  );
});

self.addEventListener('fetch', e => {
  e.respondWith(
    caches.match(e.request).then(res => {
      return res || fetch(e.request);
    })
  );
});
